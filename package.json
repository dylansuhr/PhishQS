{
  "name": "phish-qs",
  "version": "1.0.0",
  "description": "Vercel serverless functions for PhishQS tour statistics",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "generate-stats": "node Server/Scripts/generate-stats.js",
    "update-tour-dashboard": "node Server/Scripts/update-tour-dashboard.js",
    "initialize-tour-shows": "node Server/Scripts/initialize-tour-shows.js",
    "initialize-remaining-shows": "node Server/Scripts/quick-initialize-remaining-shows.js",
    "deploy": "npx vercel --prod --yes"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "vercel": "^32.5.0"
  },
  "engines": {
    "node": ">=18"
  }
}
